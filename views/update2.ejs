                    <option>Telha</option>
                    <option>Tobias Barreto</option>
                    <option>Tomar do Geru</option>
                    <option>Umbaúba</option>
                  </select>
                </div>
                <div class="col-md-2 mb-3 mb-md-0">
                  <label class="text-black" for="fcep">CEP</label>
                  <input type="text" id="fcep" class="form-control cep" name="cep">
                </div>
              </div>
              <div class="row form-group">
                <div class="col-md-4 mb-3 mb-md-0">
                  <label class="text-black" for="frua">Endereço</label>
                  <input type="text" id="frua" class="form-control" placeholder="" name="endereco">
                </div>
                <div class="col-md-4 mb-3 mb-md-0">
                  <label class="text-black" for="fcomp">Complento</label>
                  <input type="text" id="fcomp" class="form-control" placeholder="" name="complemento">
                </div> 
                <div class="col-md-2 mb-3 mb-md-0">
                  <label class="text-black" for="fcasa">Nº</label>
                  <input type="text" id="fcasa" class="form-control" placeholder="" name="ncasa">
                </div>    
                <div class="col-md-2 mb-3 mb-md-0">
                  <label class="text-black" for="fvenc">Vencimento</label>
                  <input type="number" id="fvenc" class="form-control" placeholder="" name="vencimento" max="31">
                </div>            
              </div>
              <div class="row form-group">
                <div class="col-md-2 mb-3 mb-md-0">
                  <label class="text-black">Modalidade</label>
                  <select class="form-control" data-style="btn-white btn-lg" data-width="100%" data-live-search="true" name="mod">
