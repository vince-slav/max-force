<!doctype html>
<html lang="pt-BR">
<head>
    <title>Medidas</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="css/custom-bs.css">
    <link rel="stylesheet" href="css/jquery.fancybox.min.css">
    <link rel="stylesheet" href="css/bootstrap-select.min.css">
    <link rel="stylesheet" href="fonts/icomoon/style.css">
    <link rel="stylesheet" href="fonts/line-icons/style.css">
    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <link rel="stylesheet" href="css/animate.min.css">
    <link rel="stylesheet" href="css/quill.snow.css">
    <!-- MAIN CSS -->
    <link rel="stylesheet" href="css/style.css">
</head>

<body id="top">
    <div id="overlayer"></div>
    <div class="loader">
        <div class="spinner-border text-primary" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>

    <div class="site-wrap">
        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div> <!-- .site-mobile-menu -->

        <!-- NAVBAR -->
        <header class="site-navbar mt-3">
            <div class="container-fluid">
                <div class="row align-items-center">
                    <div class="site-logo col-6"><a href="/">maxforce</a></div>

                    <nav class="mx-auto site-navigation">
                        <li class="d-lg-none"><a href="/login">Login</a></li>
                        </ul>
                    </nav>

                    <div class="right-cta-menu text-right d-flex aligin-items-center col-6">
                        <div class="ml-auto">
                            <a href="/medidas" class="btn btn-primary border-width-2 d-none d-lg-inline-block">Minhas medidas</a>
                            <a href="/treinoDoDia" class="btn btn-primary border-width-2 d-none d-lg-inline-block">Meu treino</a>
                            <a href="/contato" class="btn btn-primary border-width-2 d-none d-lg-inline-block"><span class=""></span>Fale conosco</a>
                        </div>
                        <a href="#" class="site-menu-toggle js-menu-toggle d-inline-block d-xl-none mt-lg-2 ml-3"><span
                                class="icon-menu h3 m-0 p-0 mt-2"></span></a>
                    </div>

                </div>
            </div>
        </header>

        <!-- HOME -->
        <section class="section-hero overlay inner-page bg-image" style="background-image: url('images/hero_3.jpg');"
            id="home-section">
            <div class="container">
                <div class="row">
                    <div class="col-md-7">

                    </div>
                </div>
            </div>
        </section>

        <section class="site-section">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card" style="border-style: solid; border-color: #008000; width: 450px;">
                            <div class="card-body">
                                <h4 class="card-title" style="color: #008000;">Dados Antropométricos</h4>
                                <hr style="border-color: #008000;">
                                <p class="card-text"><strong style="color: #008000;">Peso:</strong> <span
                                        class="card-text">
                                        <%= medidas.peso %> Kg
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Estatura:</strong> <span
                                        class="card-text">
                                        <%= medidas.estatura %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra Tricipal:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraTricipal %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra da Coxa:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraAbdominal %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra Toráxica:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraSubescapular %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra Suprailiaca:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraAxilarMedia %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra Abdominal:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraCoxa %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra Subescapular:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraToraxica %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Dobra Axilar Média:</strong> <span
                                        class="card-text">
                                        <%= medidas.dobraSuprailiaca %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Circumferência Braco
                                        Relaxado:</strong> <span class="card-text">
                                        <%= medidas.circumferenciaBracoRelaxado %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Circumferência Antebraco:</strong>
                                    <span class="card-text">
                                        <%= medidas.circumferenciaAntebraco %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Circumferência Cintura:</strong>
                                    <span class="card-text">
                                        <%= medidas.circumferenciaCintura %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Circumferência Abdômen:</strong>
                                    <span class="card-text">
                                        <%= medidas.circumferenciaAbdomen %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Circumferência Quadril:</strong>
                                    <span class="card-text">
                                        <%= medidas.circumferenciaQuadril %> cm
                                    </span></p>
                                <p class="card-text"><strong style="color: #008000;">Circumferência Coxa
                                        Medial:</strong> <span class="card-text">
                                        <%= medidas.circumferenciaCoxaMedial %> cm
                                    </span></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

   <%- include('includes/footer') %>
    </div>

    <!-- SCRIPTS -->
    <script src="js/jquery.min.js"></script>
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/isotope.pkgd.min.js"></script>
    <script src="js/stickyfill.min.js"></script>
    <script src="js/jquery.fancybox.min.js"></script>
    <script src="js/jquery.easing.1.3.js"></script>
    <script src="js/jquery.waypoints.min.js"></script>
    <script src="js/jquery.animateNumber.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/quill.min.js"></script>
    <script src="js/bootstrap-select.min.js"></script>
    <script src="js/custom.js"></script>
    <script src="/js/jquery.mask.min.js"></script>
    <script>
        $(".phone").mask("(00) 000000000");
    </script>
    <script>
        $(".cpf").mask("000.000.000-00");
    </script>
    <script>
        $(".cep").mask("00000-000")
    </script>
</body>

</html>